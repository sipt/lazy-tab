{
  "manifest_version": 3,
  "name": "Tab Player",
  "description": "",
  "version": "1.0",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": ["tabGroups", "tabs", "tabCapture", "activeTab"]
}
